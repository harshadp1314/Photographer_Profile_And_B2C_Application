        function ValidateURL(url) {
        //var url = document.getElementById("url").value;
            var pattern = /^(?:(?:https?|ftp):\/\/)?(?:(?!(?:10|127)(?:\.\d{1,3}){3})(?!(?:169\.254|192\.168)(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})))(?::\d{2,5})?(?:\/\S*)?$/;
        if (pattern.test(url)) {

            return true;
        }

            return false;
        }

        function ValidateFacebookURL(url) {
            //var url = document.getElementById("url").value;
            var pattern = /(?:http:\/\/)?(?:www\.)?facebook\.com\/(?:(?:\w)*#!\/)?(?:pages\/)?(?:[\w\-]*\/)*([\w\-]*)/;
            if (pattern.test(url)) {

                return true;
            }

            return false;
        }

        function ValidateYoutubeURL(url) {
            //var url = document.getElementById("url").value;
            var pattern = /^(?:https|http)\:\/\/(?:[\w]+\.)?youtube\.com\/(?:c\/|channel\/|user\/)?([a-zA-Z0-9\-]{1,})$/;
            if (pattern.test(url)) {

                return true;
            }

            return false;
        }

        function ValidateInstagramURL(url) {
            //var url = document.getElementById("url").value;
            var pattern = /(?:(?:http|https):\/\/)?(?:www\.)?(?:instagram\.com|instagr\.am)\/([A-Za-z0-9-_\.]+)/im;
            if (pattern.test(url)) {

                return true;
            }

            return false;
        }

        function ValidateGoogleMapURL(url) {
            var res = url.replace("@@", "&#64");
            //var url = document.getElementById("url").value;
            var pattern = /\/&#64([\d\.,-]+)z\//;
            //var pattern = /^https?\:\/\/(www\.)?google\.[a-z]+\/maps\/?\?([^&]+&)*(ll=-?[0-9]{1,2}\.[0-9]+,-?[0-9]{1,2}\.[0-9]+|q=[^&+])+($|&)/;
            if (pattern.test(res)) {

                return true;
            }

            return false;
        }

