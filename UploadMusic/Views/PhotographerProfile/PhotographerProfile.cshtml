        .rating-container:hover .counter {
            display: block;
        }

        .counter {
            display: none;
            float: right;
            font-weight: bold;
            margin-left: 173px;
            margin-top: 0px;
            padding-left: 174px;
            padding-right: 0px;
            background-color: rgba(0, 0, 0, 0.8);
            color: white;
            padding: 5px;
            position: absolute;
            line-height: 11px;
            top: 19%;
            width: 30px;
            height: 30px;
        }

        .rating-container .counter:before,
        .rating-container .counter:before, .rating-container .counter ::before {
            content: "";
            width: 0;
            height: 0;
            top: 50%;
            margin-top: -5px;
            display: block;
            border-color: transparent rgba(0, 0, 0, 0.8) transparent transparent;
            border-width: 5px 5px 5px 0px;
            left: -10px;
            border-style: solid;
            position: relative;
        }
