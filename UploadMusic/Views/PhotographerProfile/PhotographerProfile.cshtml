                    alert(response);
                }
            });
        }

        //Overall Rating for Photographer
        function GetRating() {
            //localStorage.clear();
            var rating = {};
            rating.PhotographerID = @Model.objPhotoGrapherDetail.PhotographerID
            $.ajax({
                url: window.location.protocol + "//" + window.location.host + "/PhotographerProfile/GetRatings",
                type: "POST",
                dataType: 'json',
                contentType: "application/json; charset=utf-8",
                data: '{rating: ' + JSON.stringify(rating) + '}',
                success: function (response) {


                    //localStorage.setItem('Rating', response.response.NoOfStars);
                    console.log("Response", response.response.NoOfStars);

                    var mainRating = response.response.NoOfStars;
                    console.log("MainRating", mainRating)
                    if (mainRating == 0) {
                        $('#avgRating').text(' NOT YET RATED ');
                    } else {
                        $('#avgRating').text(' AVERAGE RATING ');
                        $('#mainRating').text(mainRating);
                    }

                    //var rating = getRandomRating();
                    $("#rateYo").rateYo("rating", mainRating);

                    // Getter
                    var ratedFill = $("#rateYo").rateYo("option", "ratedFill"); //returns "#E74C3C"

                    // Setter
                    $("#rateYo").rateYo("option", "ratedFill", "#18ba60"); //returns a jQuery Element

                    // Getter
                    var readOnly = $("#rateYo").rateYo("option", "readOnly"); //returns true

                    // Setter
                    $("#rateYo").rateYo("option", "readOnly", true); //returns a jQuery Element


                },
                error: function (response) {
                    console.log(response);
                }
            });
        }
