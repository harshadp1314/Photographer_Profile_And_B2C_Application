            <!-- End Video Section -->
            <!-- start rating section -->
            <div class="content-inner">
                <div class="woocommerce-reviews">
                    <p>If you would like to rate this photographer, please send us a request by submitting your ratings below. Your rating will be published after verification by Pixthon team.</p>
                    <a href="@Url.Action("StarRating","StarRating",new { PhotographerID = Model.objPhotoGrapherDetail.EncodedPhotographerID,PinCode=Model.objPhotoGrapherDetail.SecurityCode })" class="btn btn-info btn-lg" style="background-color:#18ba60;">RATE THIS PHOTOGRAPHER</a>
                    </p>
                    <div id="comments">
                        <ul class="commentlist">
                            @if (Model.objrating != null)
                            {
                                foreach (var comment in Model.objrating)
                                {
                                    <li>
                                        <div class="comment_container">
                                            @comment.Name
                                            <div class="comment-text">
                                                <div class="star-rating">





                                                    @for (var starcount = 1; starcount <= ((int)comment.NoOfStars); starcount++)
                                                    {
                                                        <span><i class="fa fa-star"></i></span>

                                                    }

                                                    @{
                                                        var v = comment.NoOfStars.ToString();



                                                        if (v.IsInt() == false)
                                                        {
                                                            <span><i class="fa fa-star-half-o"></i></span>
                                                            for (var starcount = 1; starcount <= 5 - ((int)comment.NoOfStars) - 1; starcount++)
                                                            {

                                                                <span><i class="fa fa-star-o"></i></span>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            for (var starcount = 1; starcount <= 5 - ((int)comment.NoOfStars); starcount++)
                                                            {

                                                                <span><i class="fa fa-star-o"></i></span>
                                                            }
                                                        }



                                                    }

                                                </div>
                                                <p class="meta">
                                                    <strong>@comment.Title</strong> – <span>@comment.CommentedOn</span> :
                                                </p>
                                                <div class="description">
                                                    @comment.Comment
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                }
                            }



                        </ul>
                    </div>
                </div>
            </div>
            <!--End start rating section -->
        </div>
